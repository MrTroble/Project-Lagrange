<xml>
  <app>
    <string name="app_name" value="TGEngine editor"/>
    <int name="version" value="1"/>
    <!--MSAA from 1 (off) to 64 | must be a value of 2^x or 1 or 0 for auto detect-->
    <int name="msaa" value="4"/>
  </app>
  <window>
    <boolean name="fullscreen" value="false"/>
    <!--Will be ignored if fullscreen is true-->
    <boolean name="decorated" value="true" />
    <boolean name="cursor" value="true" />
    <boolean name="resizeable" value="true" />
    <boolean name="maximizable" value="true" />
    <boolean name="minimizeable" value="true" />
    <int name="height" value="600"/>
    <int name="width" value="800"/>
    <boolean name="center" value="true"/>
    <!--Will be ignored if center is true-->
    <int name="posx" value="0" />
    <int name="posy" value="0" />
    <!--Ignore end-->>
  </window>
</xml>